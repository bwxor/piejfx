{
  "extensions": [".cs"],
  "rules": [
    {
      "type": "comment",
      "regex": "//.*|/\\*[\\s\\S]*?\\*/"
    },
    {
      "type": "string",
      "regex": "@?\"(\\\\.|[^\"])*\""
    },
    {
      "type": "number",
      "regex": "\\b\\d+(\\.\\d+)?\\b"
    },
    {
      "type": "keyword",
      "regex": "\\b(abstract|as|base|bool|break|byte|case|catch|char|checked|class|const|continue|decimal|default|delegate|do|double|else|enum|event|explicit|extern|false|finally|float|for|foreach|goto|if|implicit|in|int|interface|internal|is|lock|long|namespace|new|null|object|operator|out|override|params|private|protected|public|readonly|ref|return|short|sizeof|stackalloc|static|string|struct|switch|this|throw|true|try|typeof|uint|ulong|unchecked|unsafe|ushort|using|virtual|void|volatile|while)\\b"
    },
    {
      "type": "class",
      "regex": "(?<=class\\s+)[A-Za-z_][A-Za-z0-9_]*"
    },
    {
      "type": "operator",
      "regex": "==|!=|<=|>=|&&|\\|\\||=>|[-+*/%=&|^~<>!]"
    },
    {
      "type": "punctuation",
      "regex": "[{}()\\[\\];,\\.]"
    },
    {
      "type": "indent",
      "regex": "^\\s+"
    },
    {
      "type": "function",
      "regex": "\\b[A-Za-z_][A-Za-z0-9_]*\\s*(?=\\()"
    },
    {
      "type": "identifier",
      "regex": "\\b[A-Za-z_][A-Za-z0-9_]*\\b"
    }
  ],
  "autocomplete": ["abstract","as","base","bool","break","byte","case","catch","char","checked","class","const","continue","decimal","default","delegate","do","double","else","enum","event","explicit","extern","false","finally","float","for","foreach","goto","if","implicit","in","int","interface","internal","is","lock","long","namespace","new","null","object","operator","out","override","params","private","protected","public","readonly","ref","return","short","sizeof","stackalloc","static","string","struct","switch","this","throw","true","try","typeof","uint","ulong","unchecked","unsafe","ushort","using","virtual","void","volatile","while"]
}