{
  "extensions": [".m", ".mm"],
  "rules": {
    "leading_spaces": { "type": "indent", "regex": "^\\s+" },
    "keywords": { "type": "keyword", "regex": "\\b(@interface|@implementation|@end|@class|@protocol|@selector|@synthesize|@dynamic|if|else|for|while|return|break|continue|switch|case|default|nil|YES|NO|self|super)\\b" },
    "strings": { "type": "string", "regex": "@?\"(\\\\.|[^\"\\\\])*\"" }
  }
}
