{
  "extensions": [
    ".js",
    ".mjs",
    ".cjs"
  ],
  "rules": [
    {
      "type": "comment",
      "regex": "/\\*[\\s\\S]*?\\*/"
    },
    {
      "type": "comment",
      "regex": "//.*"
    },
    {
      "type": "string",
      "regex": "\"(\\\\.|[^\"\\\\])*\"|'(\\\\.|[^'\\\\])*'|`(\\\\.|[^`\\\\])*`"
    },
    {
      "type": "number",
      "regex": "\\b(0x[0-9a-fA-F]+|\\d+\\.\\d+|\\d+)\\b"
    },
    {
      "type": "type",
      "regex": "\\b(Number|String|Boolean|Object|Array|Function|BigInt|Symbol)\\b"
    },
    {
      "type": "keyword",
      "regex": "\\b(await|break|case|catch|class|const|continue|debugger|default|delete|do|else|enum|export|extends|false|finally|for|function|if|import|in|instanceof|let|new|null|return|super|switch|this|throw|true|try|typeof|var|void|while|with|yield)\\b"
    },
    {
      "type": "class",
      "regex": "(?<=\\bclass\\s+)[A-Za-z_][A-Za-z0-9_]*"
    },
    {
      "type": "function",
      "regex": "\\bfunction\\s+[A-Za-z_][A-Za-z0-9_]*|\\b[A-Za-z_][A-Za-z0-9_]*\\s*(?=\\()"
    },
    {
      "type": "operator",
      "regex": "\\+\\+|--|==|!=|===|!==|<=|>=|&&|\\|\\||\\?\\?|<<|>>|>>>|[-+*/%=&|^~<>!]"
    },
    {
      "type": "punctuation",
      "regex": "[{}()\\[\\];,\\.]"
    },
    {
      "type": "indent",
      "regex": "^\\s+"
    },
    {
      "type": "identifier",
      "regex": "\\b[A-Za-z_][A-Za-z0-9_]*\\b"
    }
  ],
  "autocomplete": ["Number","String","Boolean","Object","Array","Function","BigInt","Symbol","await","break","case","catch","class","const","continue","debugger","default","delete","do","else","enum","export","extends","false","finally","for","function","if","import","in","instanceof","let","new","null","return","super","switch","this","throw","true","try","typeof","var","void","while","with","yield"]
}