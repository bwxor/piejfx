{
  "extensions": [
    ".json"
  ],
  "rules": [
    {
      "type": "string",
      "regex": "\"(\\\\.|[^\"\\\\])*\""
    },
    {
      "type": "number",
      "regex": "\\b-?\\d+(\\.\\d+)?([eE][+-]?\\d+)?\\b"
    },
    {
      "type": "boolean",
      "regex": "\\b(true|false|null)\\b"
    },
    {
      "type": "punctuation",
      "regex": "[{}\\[\\]:,]"
    }
  ]
}