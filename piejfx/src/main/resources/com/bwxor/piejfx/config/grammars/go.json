{
  "extensions": [
    ".go"
  ],
  "rules": [
    {
      "type": "comment",
      "regex": "//.*|/\\*[\\s\\S]*?\\*/"
    },
    {
      "type": "string",
      "regex": "`[\\s\\S]*?`|\"(\\\\.|[^\"\\\\])*\""
    },
    {
      "type": "number",
      "regex": "\\b\\d+(\\.\\d+)?\\b"
    },
    {
      "type": "keyword",
      "regex": "\\b(break|case|chan|const|continue|default|defer|else|for|func|go|goto|if|import|interface|map|package|range|return|select|struct|switch|type|var)\\b"
    },
    {
      "type": "function",
      "regex": "(?<=\\bfunc\\s+)[A-Za-z_][A-Za-z0-9_]*"
    },
    {
      "type": "operator",
      "regex": "==|!=|<=|>=|:=|[-+*/%=&|^~<>!]"
    },
    {
      "type": "punctuation",
      "regex": "[{}()\\[\\];,\\.]"
    },
    {
      "type": "indent",
      "regex": "^\\s+"
    },
    {
      "type": "identifier",
      "regex": "\\b[A-Za-z_][A-Za-z0-9_]*\\b"
    }
  ]
}