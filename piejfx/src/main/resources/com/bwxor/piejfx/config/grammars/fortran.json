{
  "extensions": [
    ".f",
    ".f90",
    ".f95"
  ],
  "rules": [
    {
      "type": "comment",
      "regex": "!.*"
    },
    {
      "type": "string",
      "regex": "'([^']|'')*'"
    },
    {
      "type": "number",
      "regex": "\\b\\d+(\\.\\d+)?\\b"
    },
    {
      "type": "keyword",
      "regex": "\\b(program|end|if|then|else|do|while|select|case|module|use|implicit|none|contains|subroutine|function|return|stop)\\b"
    },
    {
      "type": "operator",
      "regex": "\\.eq\\.|\\.ne\\.|\\.lt\\.|\\.gt\\.|\\.le\\.|\\.ge\\.|[-+*/=]"
    },
    {
      "type": "punctuation",
      "regex": "[(),:]"
    },
    {
      "type": "indent",
      "regex": "^\\s+"
    },
    {
      "type": "function",
      "regex": "\\b[A-Za-z_][A-Za-z0-9_]*\\s*(?=\\()"
    },
    {
      "type": "identifier",
      "regex": "\\b[A-Za-z_][A-Za-z0-9_]*\\b"
    }
  ],
  "autocomplete": ["program","end","if","then","else","do","while","select","case","module","use","implicit","none","contains","subroutine","function","return","stop"]
}