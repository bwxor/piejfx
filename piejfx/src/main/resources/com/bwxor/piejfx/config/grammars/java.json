{
  "extensions": [".java"],
  "rules": [
    {
      "type": "comment",
      "regex": "(\\/\\*[\\s\\S]*?\\*\\/|\\/\\/.*)"
    },
    {
      "type": "string",
      "regex": "\"([^\"\\\\]|\\\\.)*\""
    },
    {
      "type": "char",
      "regex": "'([^'\\\\]|\\\\.)'"
    },
    {
      "type": "boolean",
      "regex": "\\b(true|false|null)\\b"
    },
    {
      "type": "keyword",
      "regex": "\\b(abstract|assert|boolean|break|byte|case|catch|char|class|const|continue|default|do|double|else|enum|extends|final|finally|float|for|goto|if|implements|import|instanceof|int|interface|long|native|new|package|private|protected|public|return|short|static|strictfp|super|switch|synchronized|this|throw|throws|transient|try|void|volatile|while|var)\\b"
    },
    {
      "type": "number",
      "regex": "\\b\\d+([.]\\d+)?([eE][+-]?\\d+)?[dfDF]?\\b|\\b0x[a-fA-F0-9]+\\b"
    },
    {
      "type": "annotation",
      "regex": "@\\w+"
    },
    {
      "type": "function",
      "regex": "\\b\\w+(?=\\()"
    },
    {
      "type": "type",
      "regex": "\\b[A-Z]\\w*\\b"
    },
    {
      "type": "operator",
      "regex": "[=+\\-*/%&|^~!<>?:]+"
    },
    {
      "type": "punctuation",
      "regex": "[.,;(){}\\[\\]]"
    }
  ],
  "autocomplete": [
    "abstract","assert","boolean","break","byte","case","catch","char","class","const","continue","default","do","double","else","enum","extends","final","finally","float","for","goto","if","implements","import","instanceof","int","interface","long","native","new","package","private","protected","public","return","short","static","strictfp","super","switch","synchronized","this","throw","throws","transient","try","void","volatile","while","var"
  ]
}