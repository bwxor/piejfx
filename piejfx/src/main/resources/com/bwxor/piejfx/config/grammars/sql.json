{
  "extensions": [
    ".sql"
  ],
  "rules": [
    {
      "type": "comment",
      "regex": "/\\*[\\s\\S]*?\\*/"
    },
    {
      "type": "comment",
      "regex": "--.*"
    },
    {
      "type": "string",
      "regex": "'([^']|'')*'"
    },
    {
      "type": "number",
      "regex": "\\b\\d+(\\.\\d+)?\\b"
    },
    {
      "type": "keyword",
      "regex": "\\b(SELECT|INSERT|UPDATE|DELETE|FROM|WHERE|JOIN|INNER|LEFT|RIGHT|FULL|ON|GROUP|BY|HAVING|ORDER|LIMIT|OFFSET|DISTINCT|AS|AND|OR|NOT|IN|IS|NULL|LIKE|BETWEEN|EXISTS|CREATE|ALTER|DROP|TRUNCATE|TABLE|VIEW|INDEX|DATABASE|PRIMARY|KEY|FOREIGN|REFERENCES|UNIQUE|CHECK|DEFAULT)\\b"
    },
    {
      "type": "type",
      "regex": "\\b(INT|INTEGER|SMALLINT|BIGINT|DECIMAL|NUMERIC|FLOAT|REAL|DOUBLE|CHAR|VARCHAR|TEXT|DATE|TIME|TIMESTAMP|BOOLEAN|BLOB)\\b"
    },
    {
      "type": "function",
      "regex": "\\b[A-Za-z_][A-Za-z0-9_]*\\s*(?=\\()"
    },
    {
      "type": "operator",
      "regex": "=|<>|!=|<|>|<=|>=|\\+|\\-|\\*|/|%"
    },
    {
      "type": "punctuation",
      "regex": "[(),.;]"
    },
    {
      "type": "indent",
      "regex": "^\\s+"
    },
    {
      "type": "identifier",
      "regex": "\\b[A-Za-z_][A-Za-z0-9_]*\\b"
    }
  ],
  "autocomplete": ["SELECT","INSERT","UPDATE","DELETE","FROM","WHERE","JOIN","INNER","LEFT","RIGHT","FULL","ON","GROUP","BY","HAVING","ORDER","LIMIT","OFFSET","DISTINCT","AS","AND","OR","NOT","IN","IS","NULL","LIKE","BETWEEN","EXISTS","CREATE","ALTER","DROP","TRUNCATE","TABLE","VIEW","INDEX","DATABASE","PRIMARY","KEY","FOREIGN","REFERENCES","UNIQUE","CHECK","DEFAULT","INT","INTEGER","SMALLINT","BIGINT","DECIMAL","NUMERIC","FLOAT","REAL","DOUBLE","CHAR","VARCHAR","TEXT","DATE","TIME","TIMESTAMP","BOOLEAN","BLOB"]
}